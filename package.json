{
  "name": "admin-starter",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build:stage": "vue-tsc --noEmit && vite build --mode staging",
    "build:prod": "vue-tsc --noEmit && vite build",
    "build": "vite build",
    "preview:stage": "pnpm build:stage && vite preview",
    "preview:prod": "pnpm build:prod && vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "husky install && vite optimize",
    "release": "git add . && npx bumpp patch -y --commit --push --all --tag -r",
    "test": "vitest"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.3.1",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "@wangeditor/plugin-upload-attachment": "^1.1.0",
    "axios": "1.6.5",
    "dayjs": "1.11.10",
    "echarts": "^5.4.3",
    "element-plus": "2.6.3",
    "husky": "8.0.3",
    "image-conversion": "^2.1.1",
    "js-cookie": "3.0.5",
    "lingman": "^1.0.25",
    "lodash-es": "4.17.21",
    "mitt": "3.0.1",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "path-browserify": "1.0.1",
    "path-to-regexp": "6.2.1",
    "pinia": "2.1.7",
    "print-js": "file:./packages/print-js",
    "screenfull": "6.0.2",
    "vue": "3.4.15",
    "vue-router": "4.2.5",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@djie/eslint-config": "2.1.5",
    "@djie/unocss": "^1.0.32",
    "@djie/vite-plugin-vue-inspector": "^6.0.0",
    "@types/js-cookie": "3.0.6",
    "@types/lodash-es": "4.17.12",
    "@types/node": "20.11.5",
    "@types/nprogress": "0.2.3",
    "@types/path-browserify": "1.0.2",
    "@unocss/transformer-directives": "^0.59.2",
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitejs/plugin-vue": "5.0.3",
    "@vitejs/plugin-vue-jsx": "3.1.0",
    "@vue/test-utils": "2.4.3",
    "eslint": "8.57.0",
    "jsdom": "23.2.0",
    "lint-staged": "15.2.0",
    "prettier": "3.2.4",
    "sass": "1.70.0",
    "typescript": "5.3.3",
    "unocss": "0.58.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "5.2.8",
    "vite-plugin-inspect": "^0.8.1",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-plugin-vue-devtools": "^7.0.11",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-svg-loader": "5.1.0",
    "vitest": "1.2.1",
    "vue-tsc": "1.8.27"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,scss,html,md}": [
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ]
  }
}
